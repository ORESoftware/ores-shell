{
  "compilerOptions": {

    "declaration": false,
    "allowJs": false,
    "allowUnreachableCode": false,
    "alwaysStrict": true,
    "baseUrl": "ts",
    "lib": [
      "es2017",
      "es2018",
      "es2019"
    ],
    "resolveJsonModule": true,
    "sourceMap": false,
    "removeComments": true,
    "skipLibCheck": true,
    "target": "es2018",
    "module": "commonjs",
    "noImplicitAny": false,
    "outDir": "ts/dist"
  },
  "include": [
    "ts/src"
  ]
}